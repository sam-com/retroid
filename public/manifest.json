{
	"$schema": "https://json.schemastore.org/web-manifest-combined.json",
	"background_color": "#161b1d",
	"description": "Emulation library in the web browser",
	"display": "standalone",
	"orientation": "landscape",
	"icons": [
		{
			"purpose": "any",
			"sizes": "200x200",
			"src": "/retroid/favicon.svg",
			"type": "image/svg"
		},
		{
			"purpose": "any",
			"sizes": "192x192",
			"src": "/retroid/apple-touch-icon.png",
			"type": "image/png"
		},
		{
			"purpose": "maskable",
			"sizes": "512x512",
			"src": "/retroid/retroid.png",
			"type": "image/png"
		},
		{
			"purpose": "maskable",
			"sizes": "250x250",
			"src": "/retroid/retroid.svg",
			"type": "image/svg"
		}
	],
	"id": "/retroid/",
	"lang": "en-us",
	"name": "Retroid",
	"short_name": "retroid",
	"start_url": "/retroid/",
	"theme_color": "#000000"
}
